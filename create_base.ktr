    <sql>drop table if exists bookings.dim_calendar cascade;
create table bookings.dim_calendar as
	with d as
	(select generate_series
	        ('2010-01-01',
	         'now',
	         '1 day'::interval)::date "date")
	 select *,
		    extract ("isodow" from "date")::int day_of_week,
		    extract ("day" from "date")::int day_of_month,
		    extract ("week" from "date")::int week_of_year,
		    extract ("month" from "date")::int "month",
		    extract ("year" from "date")::int "year"
from d;


drop table if exists bookings.dim_passengers cascade;
create table bookings.dim_passengers (
	id serial8 primary key,
	passenger_id char(11) not null unique,
	passenger_name text not null,
	phone text not null,
	email text null,
	constraint id_digit check (passenger_id ~ ('[0-9]{4} [0-9]{6}')),
	constraint phone_digit check (phone ~ ('\+[0-9]{8,20}'))
);


drop table if exists bookings.dim_aircrafts cascade;
create table bookings.dim_aircrafts (
	id serial primary key,
	aircraft_code char(3) not null unique,
	model text not null,
	"range" int not null,
	constraint range_speed check (0 &lt; "range" and "range" &lt; 30000)
);


drop table if exists bookings.dim_airports cascade;
create table bookings.dim_airports (
	id serial primary key,
	airport_code char(3) not null unique,
	airport_name text not null,
	city text not null,
	longitude float8 not null,
	latitude float8 not null,
	timezone text not null,
	constraint code_no_digit check (airport_code ~ ('[^0-9][^0-9][^0-9]')),
	constraint continent check ((split_part(timezone, '/', 1)) in ('Europe', 'Asia', 'America')),
	unique (longitude, latitude)
);


drop table if exists bookings.dim_tariff cascade;
create table bookings.dim_tariff (
	id serial primary key,
	tariff text not null unique,
	constraint tariff_in_list check (tariff in ('Economy', 'Business', 'Comfort'))
);


drop table if exists bookings.fact_flights cascade;
create table bookings.fact_flights (
	id serial8 not null primary key,
	passenger int8 not null,
	actual_departure timestamp not null,
	actual_arrival timestamp not null,
	delay_departure int null,
	delay_arrival int null,
	aircraft int not null,
	departure_airport int not null,
	arrival_airport int not null,
	tariff int not null,
	amount numeric(10, 2) not null,
	constraint passenger_fkey foreign key (passenger) references bookings.dim_passengers(id),
	constraint aircraft_fkey foreign key (aircraft) references bookings.dim_aircrafts(id),
	constraint departure_airport_fkey foreign key (departure_airport) references bookings.dim_airports(id),
	constraint arrival_airport_fkey foreign key (arrival_airport) references bookings.dim_airports(id),
	constraint tariff_fkey foreign key (tariff) references bookings.dim_tariff(id), 
	constraint airports_diff check (departure_airport != arrival_airport),
	constraint date_departure check (actual_departure &lt;= now()),
	constraint amount_cost check (amount > 0)
);

create or replace view bookings.view_for_work as
select f.flight_id,
	f.scheduled_departure,
	f.scheduled_arrival,
	f.departure_airport,
	f.arrival_airport,
	a.aircraft_code,
	f.actual_departure,
	f.actual_arrival,
	tf.fare_conditions,
	tf.amount,
	t.passenger_id
from bookings.flights f
join bookings.ticket_flights tf on tf.flight_id = f.flight_id
join bookings.tickets t on t.ticket_no = tf.ticket_no
join bookings.aircrafts a on a.aircraft_code = f.aircraft_code
where f.actual_arrival is not null;
</sql>
